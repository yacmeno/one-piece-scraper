{
	"name": "One Piece JB",
	"version": "1.0",
	"description": "Display the latest One Piece release from Jaimini's Box",
	"permissions": ["https://jaiminisbox.com/reader/series/one-piece-2/",
									"storage"],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"icons": {
		"16": "images/jb-logo.png",
		"48": "images/jb-logo.png",
		"128": "images/jb-logo.png"
	},
	"manifest_version": 2
}
